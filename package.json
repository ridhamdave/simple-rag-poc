{
  "name": "davemind",
  "version": "1.0.0",
  "description": "DaveMind - A RAG (Retrieval-Augmented Generation) system with knowledge base and chat interface",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup-vectors": "node scripts/setup-vectors.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "faiss-node": "^0.4.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "mongoose": "^8.18.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "rag",
    "vector-database",
    "chat",
    "knowledge-base",
    "ai",
    "gemini",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Ridham Dave",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ridhamdave/davemind.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
